@model List<KeyValuePair<int, string>>
<select class="form-control" v-model="@ViewData["field"]" style="@ViewData["style"]">
    <option value="">@ViewData["title"]</option>
    @{
        if (Model.Any())
        {
            foreach (var item in Model)
            {
                <option value="@item.Key">@item.Value</option>
            }
        }
    }
</select>
