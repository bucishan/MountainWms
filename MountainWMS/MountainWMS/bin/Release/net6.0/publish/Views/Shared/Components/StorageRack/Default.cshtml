@model List<M.Core.Entity.Wms_storagerack>
<select class="form-control" v-model="@ViewData["field"]" style="@ViewData["style"]">
    <option value="">@ViewData["title"]</option>
    @{
        if (Model.Any())
        {
            foreach (var item in Model)
            {
                <option value="@item.StorageRackId">@(item.StorageRackNo + "|" + item.StorageRackName)</option>
            }
        }
    }
</select>
