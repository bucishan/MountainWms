@model List<M.Core.Entity.Sys_dict>
<select class="form-control" v-model="@ViewData["field"]" style="width:120px;margin-left:10px;">
    <option value="">全部类型</option>
    @{
        if (Model.Any())
        {
            foreach (var item in Model)
            {
                <option value="@item.DictId">@item.DictName</option>
            }
        }
    }
</select>
